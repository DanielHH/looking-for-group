<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <size
                android:height="50dp"
                android:width="50dp"/>
            <solid
                android:width="1dp"
                android:color="@color/colorAccent"/>
        </shape>
    </item>

    <item>
        <rotate
            android:fromDegrees="-35"
            android:pivotX="50%"
            android:pivotY="50%">

            <shape
                android:shape="line">
            <stroke
                android:width="1dp"
                android:color="#FFFFFF"/>
            </shape>
        </rotate>
    </item>
</layer-list>